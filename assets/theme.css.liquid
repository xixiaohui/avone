.logo-bar__link { display:block; margin:auto; }
.logo-bar__link .logoImg { background-size:contain; background-repeat:no-repeat; background-position:center; }

{% comment %}/*========= Instagram Sec =========*/{% endcomment %}
.insta-shop { margin:0 -10px; }
.home-instagram .instagram-des { margin-bottom:25px; }
.home-instagram .instafeed { position:relative; width:100%; text-align:center; }
.home-instagram .instafeed:not(.carousel ) { display:flex; flex-wrap:wrap; }
.home-instagram .instafeed .insta-img a { position:relative; padding-bottom:100%; display:block; overflow:hidden; }
.home-instagram .instafeed .insta-img img { display:block; width:100%; position:absolute; top:50%; left:50%; transform:translate(-50%, -50%); }
.home-instagram .grid-sp .insta-img { padding:0 5px 10px; }
.home-instagram .followus { text-align:center; padding:20px 0; }
.home-instagram .followus .btn { position:relative; z-index:555; }


{% comment %}/*========= video =========*/{% endcomment %}
.video-sec { position:relative; font-size:0px; }
.video-text { position:absolute; }
.video-text .ttl { font-size:{{ settings.video_ttl}}px; display:block; margin-bottom:15px; }
@media only screen and (min-width:767px) {
    .video-text.left { left:7%; text-align:left; top:50%; }
    .video-text.leftbottom { left:7%; text-align:left; bottom:10%; transform:none; top:inherit; }
    .video-text.right { right:7%; text-align:right; top:50%; }
    .video-text.rightbottom { right:7%; text-align:right; bottom:10%; transform:none; top:inherit; }
    .video-text.topcenter { left:50%; top:{% if settings.home_classic %}{{settings.header_height | plus: 30}}px{% else %}5%{% endif %}; transform:translateX(-50%); text-align:center; }
    .video-text.bottomcenter { top:inherit; left:50%; bottom:10%; transform:translateX(-50%); text-align:center; }
    .video-text.center { left:50%; top:50%; transform:translate(-50%, -50%); text-align:center; }
}
@media only screen and (max-width:766px) {
	.video-text { top:50%; transform:translateY(-50%); text-align:center; width:100%; }
	.video-text .ttl { font-size:{{ settings.video_ttl | minus: 3}}px; }
}


.simple-text { letter-spacing:normal; }
.simple-text a { text-decoration:underline; }
.simple-text span { display:block; margin-bottom:20px; }
@media only screen and (max-width:766px) {
	.simple-text { font-size:16px; padding:0 15px; }
}


.wrap-block { margin:0 auto; display:block; text-align:center; }
.wrap-block .simple-text { padding:0; }
.wrap-block a { margin-top:20px; }


{% comment %}/*========= Collection Page =========*/{% endcomment %}
.template-collection .main-col.right { float:right; }

.collection-image { margin-bottom:20px; }
.collection-title { font-size:{{settings.cl_bnr_title}}px; text-transform:uppercase; font-weight:700; margin-bottom:0; }
.collection-description { font-size:{{settings.cl_bnr_des}}px; }

.clPageBnr { justify-content:center; align-items:center; padding:20px 30px; position:relative; background-position:50% 50%; background-repeat:no-repeat; background-size:cover; }
.clPageBnr.info-below { padding:0; }
.clPageBnr.info-over { margin-bottom:20px; }
.clPageBnr.text-left { justify-content:flex-start; }

.info-below .collection-title  {margin-bottom:12px;}

.catBanners { margin-bottom:25px; }
.catBanners .slideshow > .slideshow__slide:not(:first-child) { display:none; }


.sidebar { transition:all 0.3s ease-in-out; }

.closeFilter { z-index:333; color:{{settings.color_body_text}}; font-size:17px; line-height:34px; height:35px; width:35px; text-align:center; 
     cursor:pointer; position:absolute; top:5px; right:5px; }
.sidebar .sidebar_widget { margin-bottom:30px; }
.sidebar .widget-title { color:{{settings.sidebar_ttls}}; font-size:{{settings.sidebar_title}}px; margin-bottom:10px; font-weight:700; letter-spacing:0.2px; position:relative; }

@media only screen and (min-width:1024px) {
  .sidebar.popup { width:300px; height:100%; padding:15px; position:fixed; top:0; left:-310px; z-index:335; overflow:auto; background-color:{{settings.color_body_bg}}; box-shadow:0 0 10px rgba(0, 0, 0, 0.1); }
  .sidebar.popup.active { left:0; }
  
}
@media only screen and (max-width:1024px) {
  .sidebar_tags { overflow:auto; max-height:100%;}
  .filterbar { width:250px; height:100%; padding:0; position:fixed; top:0; left:-260px; z-index:444; background-color:{{settings.color_body_bg}}; box-shadow:0 0 10px rgba(0,0,0,0.1); }
  .filterbar.active { left:0; }
  .filterbar .sidebar_widget { padding:15px 15px 0 15px; margin-bottom:15px; }
}

ul.sidebar_cate .grid__item { float:none; }
ul.sidebar_cate li { padding:4px 0; }
ul.sidebar_cate a { color:{{settings.sidebar_links}}; }
ul.sidebar_cate a:hover { color:{{settings.sidebar_links_hv}}; }
ul.sidebar_cate ul { margin:0 0 0 15px; }

.filterBox ul.twoCall { column-count:2; column-gap:12px; }
.filterBox ul.opt li { padding:3px 0; }
.filterBox ul.opt li label { font-weight:400; font-size:{{settings.sidebar_option}}px; }

.filterBox .clr li { display:inline-block; padding:0; margin:0 5px 5px 0; position:relative; }
.filterBox .clr .clrbox { font-size:11px; display:block; height:18px; width:18px; line-height:18px; border-radius:0; }
.filterBox .clr .clrbox:before { position:absolute; width:100%; text-align:center; color:#fff; font-family:"adorn-icons"; }
.filterBox .clr .clrbox.rounded { border-radius:50%; }
.filterBox .clr .clrbox.rounded_rectangle { border-radius:4px; }
.filterBox .clr .clrbox.medium { font-size:13px; height:30px; width:30px; line-height:30px; }
.filterBox .clr .clrbox.large { font-size:14px; height:40px; width:40px; line-height:40px; }
.filterBox .clr .clrbox.swtwhite { border:1px solid #eee; }
.filterBox .clr .clrbox.swtwhite:before { color:#000; }
.filterBox .clr input:checked + .clrbox:before { content:'\ea7f'; }
.filterBox .clr li:hover .tooltip-label { opacity:1; visibility:visible; bottom:calc(100% + 7px); }

.crFilters .actFilter { cursor:pointer; display:inline-block; margin:0 3px 0 0; padding:3px 6px; background:#000; color:#fff; font-size:11px; }
.crFilters .ad { font-size:11px; vertical-align:baseline; padding-left:5px; }
.crFilters .btn-link { display:inline-block; margin-top:10px; font-size:11px; text-decoration:underline; }

.side-pr-slider item:not(:first-child) { display:none; }
.side-pr-slider .grid-view-item { margin-bottom:0; }
.side-pr-slider .slick-prev { left:-12px; }
.side-pr-slider .slick-next { right:-12px; }

.sidebar .static-banner { border:0 none; }


.filters-toolbar { display:flex; flex-wrap:wrap; align-items:center; justify-content:space-between; margin-bottom:20px; }
.filters-toolbar select { font-size:12px; height:auto; max-width:100px; min-width:inherit; margin:0; padding:5px 25px 5px 5px; border:0 none; }
.filters-toolbar .change-view { font-size:18px; line-height:1; background:none; opacity:0.5; border:0; padding:0 3px; }
.filters-toolbar .change-view--active { opacity:1; }
.btn-filter { border-radius:3px; text-transform:uppercase; padding:2px 12px; line-height:normal; height:auto; font-size:12px; letter-spacing:0; margin-right:15px; }
.btn-filter .ad { font-size:13px; vertical-align:middle; margin-bottom:3px; }
.flbarCount { font-size:{{settings.type_base_size | minus:1 }}px; margin:0 auto; }

@media only screen and (max-width:766px) {
	.btn-filter { width:100%; padding:5px; margin:0 0 10px; }
  	.flbarCount { font-size:{{settings.type_base_size | minus:2 }}px; }
	.medium-up--hide .medium-up--show { display:block !important; }
}


{% comment %}/*========= Product Detail Page =========*/{% endcomment %}
.product-single__photos { margin-bottom:45px; }
@media only screen and (max-width:766px) {
	.product-single__photos { margin-bottom:25px; }
}
{% if settings.product_latyout == "style3" %}
@media only screen and (min-width:766px) {
body.template-product .product-single__photos { position:sticky; position:-webkit-sticky; top:{{settings.sticky_header_height | plus: "20"}}px; }
}
{% endif %}

.prod-large-img { position:relative; z-index:1; }
{% if settings.product_latyout == "style1" or settings.product_latyout == "style2" or settings.product_latyout == "style3" or settings.product_latyout == "style6" %}
	@media only screen and (min-width:766px){
  		.prod-large-img {
          	{% if settings.thumbnail_pos == "left" %}float:right; width:85%;{% endif %}
    		{% if settings.thumbnail_pos == "right" %}float:left; width:85%;{% endif %}
        }
	}
	.prod-large-img .slick-prev { left:0; background:none; }
  	.prod-large-img .slick-next { right:0; background:none; }
{% endif %}

.style2 .prod-large-img .slick-prev{left:10px;}

.zoomcr:hover { cursor:crosshair; }
.product-single__photo { min-height:1px; position:relative; overflow:hidden;
  {% if settings.product_latyout == "style2" %}width:50%; margin-left:10px;{% endif %}
  {% if settings.product_latyout == "style5" %}width:calc(50% - 5px);{% endif %}
  {% if settings.product_latyout == "style7" %}@include transition(all 0.3s ease-in-out);{% endif %}
}
{% if settings.product_latyout == "style4" or settings.product_latyout == "style5" %}
  @media only screen and (min-width:766px){
    .product-single__photo { margin-bottom:10px; opacity:1 !important; }
    .product-single__photo:last-child { margin-bottom:0; }
  }
{% else %}
	.primgSlider .product-single__photo:not(.slick-slide):not(:first-child) { display:none; }
{% endif %}
{% if settings.product_latyout == "style7" %}
	.product-single__photo:not(.slick-current) { opacity:0.5; }
{% endif %}

{% if settings.product_latyout == "style2" %}
	.primgSlider { margin-left:-10px; }
{% elsif settings.product_latyout == "style5" %}
	.primgSlider { display:flex; flex-wrap:wrap; justify-content:space-between; }
	.style5 .product-btn { left:10px; right:auto; }
{% endif %}

.prMedia { position:relative; }
.prMedia .shopify-model-viewer-ui, .prMedia model-viewer { position:absolute; top:0; left:0; width:100%; height:100%; }

.product-single__media video { width:100%; }

.product-btn { position:absolute; right:10px; bottom:10px; }
.product-btn .btn { width:40px; float:left; height:35px; line-height:30px; padding:0; position:relative; margin-right:2px; }
.product-btn .ad { font-size:18px; }
.product-btn .popup-video .ad { font-size:22px; }
@media only screen and (min-width:1025px) {
	.product-btn .btn:hover .tooltip-label { opacity:1; visibility:visible; bottom:calc(100% + 7px); }
}

{% if settings.product_latyout == "style1" or settings.product_latyout == "style2" or settings.product_latyout == "style3" or settings.product_latyout == "style6" %}
	@media only screen and (min-width:1025px){
      	.thumbnails-wrapper {
      		{% if settings.thumbnail_pos == "left" %}float:left; width:15%; padding-right:10px;{% endif %}
    		{% if settings.thumbnail_pos == "right" %}float:right; width:15%; padding-left:10px;{% endif %}
			{% if settings.thumbnail_pos == "bottom" %}padding-top:10px; margin:0 -5px;{% endif %}
        }
	}
{% endif %}
.thumbnails-wrapper .prThumb { display:block; position:relative;
  {% if settings.thumbnail_pos != "bottom" %}
  	display:block; margin-bottom:10px;
  {% else %}
  	display:inline-block; width:16.66%; padding:0 5px; vertical-align:top;
  {% endif %}
}
.thumbnails-wrapper .prThumb:not(.slick-slide):not(:first-child) { display:none; }
.thumbnails-wrapper .slick-current { opacity:0.5; }

{% if settings.thumbnail_pos != "bottom" %}
  @media only screen and (min-width:1025px) {
      .thumbnails-wrapper .slick-prev { top:0; left:50%; transform:translate(-50%, 0); }
      .thumbnails-wrapper .slick-prev:before { content:"\e966"; }
      .thumbnails-wrapper .slick-next { top:inherit; bottom:0; left:50%; transform:translate(-50%, 0); }
      .thumbnails-wrapper .slick-next:before { content:"\e95f"; }
  }
{% else %}
  	.thumbnails-wrapper .slick-prev { left:0; }
	.thumbnails-wrapper .slick-next { right:0; }
{% endif %}

@media only screen and (max-width:1024px) {
	.thumbnails-wrapper .product-single__thumbnails { margin:6px -3px 0; }
	.thumbnails-wrapper .prThumb { display:inline-block; width:20%; padding:0 3px; margin:0; vertical-align:top; }
}

.product-single__thumbnail { display:block; padding-bottom:{{settings.primg_ratio}}%; background-repeat:no-repeat; background-position:{{settings.primg_position | replace: "_"," "}}; background-size:{{settings.primg_size}}; }

.thumbnails-wrapper .ad-play { color:#fff; text-shadow:0px 0 2px rgba(0, 0, 0, 0.5); font-size:17px; margin:-10px 0 0 -7px; position:absolute; top:50%; left:50%; }

.product-single .social-sharing { padding-top:20px; text-align:center; }
.product-single .social-sharing .share-title { display:none; }

.product-single__meta { margin-bottom:45px; position:relative;
     {% if settings.product_latyout == "style4" or settings.product_latyout == "style5" %}
    	position: sticky; position:-webkit-sticky; top:{{settings.sticky_header_height | plus: "20"}}px;
     {% endif %}
     {% if settings.product_latyout == "style7" %}
    	max-width:500px; margin:0 auto 50px;
     {% endif %}
}

.style7 .product-single__price {justify-content:center;}

h1.product-single__title { color:{{ settings.clr_prod_title}}; text-transform:{{ settings.prod_title_transform}}; font-weight:{{settings.prod_font_weight}}; font-size:{{ settings.Prod_name_font_size}}px; margin-bottom:15px; }
@media only screen and (max-width:1024px) {
	h1.product-single__title { font-size:{{ settings.Prod_name_font_size | times: 0.85 | floor }}px; }
  .product-single__meta { margin-bottom:25px;}
}

.instock { color:{{settings.instock}}; }
.outstock, .preorder { color:{{settings.outstock}}; }

.product-info { color:{{ settings.color_body_text | color_lighten: 20 }}; margin-bottom:20px; }
.product-info > div { display:inline-block; margin:0 25px 0 0; }
.product-info > p { margin:0 0 5px; display:block; }
.product-info span.instock { color:#76bd1c; }
.product-info span.outstock { color:#ff9f3b; }
.product-info a { color:{{ settings.color_body_text | color_modify: 'alpha', 0.7 }}; }
.product-info a:hover { color:{{ settings.color_body_text }}; opacity:1; text-decoration:underline; }

{%- if settings.fake_orders -%}
.orderMsg { margin-bottom:20px; }
.orderMsg .ad { vertical-align:middle; margin-right:2px; font-size:18px; }
{%- endif -%}

.product-single__price { line-height:1.4; align-items:center; flex-wrap:wrap; margin-bottom:20px; }
.product-single__price > * { margin-right:10px; }
.product-single__price .product-price__price { color:{{settings.rprice}}; font-size:{{ settings.Prod_price_font_size}}px; font-weight:{{ settings.prod_price_font_weight}}; }
.product-single__price s.product-price__price { color:{{settings.sprice}}; font-size:{{ settings.Prod_price_font_size | times: 0.85 | floor}}px; font-weight:400; margin-right:10px; }
@media only screen and (max-width:766px) {
	.product-single__price .product-price__price { font-size:{{ settings.Prod_price_font_size | times: 0.85 | floor}}px; }
	.product-single__price s.product-price__price { font-size:{{ settings.Prod_price_font_size | times: 0.75 | floor}}px; }
}
{% if settings.show_saleoff %}
.discount-badge .off { color:{{settings.sprice}}; }
{% endif %}

{% if settings.countdown %}
.countdown-lbl { margin-bottom:5px; }
.prcountdown { margin:0 -4px 30px; }
.prcountdown > span { color:{{ settings.countdown_text }}; font-weight:700; min-width:60px; text-align:center; display:inline-block; padding:10px; border:1px solid {{ settings.countdown_border}}; background:{{ settings.countdown_text_bg}}; font-size:20px; line-height:1; margin:0 3px; }
.prcountdown > span span { font-size:11px; letter-spacing:1px; font-weight:400; display:block; text-align:center; text-transform:uppercase; padding-top:5px; }
{% endif %}

{% if settings.show_short_desc_single %}
.product-single__description { margin-bottom:20px; }
.product-single__description ul { margin:20px 0 20px 20px; }
{% if settings.product_latyout != "style7" %}.product-single__description ul { text-align:left; }{% endif %}
.moredetail { margin:-10px 0 25px; }
.moredetail a {text-decoration:underline;}
{% endif %}

{%- if settings.quantity_msg -%}
#quantity_message { color:{{ settings.product_qunt_text_clr }}; padding:1px 12px; margin-bottom:20px; display:inline-block; background:{{ settings.product_qunt_bg}}; }
{% endif %}

.product-form { display:flex; flex-wrap:wrap; margin:0 -5px 10px; }
.product-form__item { flex:1 1 200px; margin-bottom:10px; padding:5px; }
.product-form__item label { display:block; }
.product-form__input { display:block; width:100%; margin:0; }

.sizelink { margin-left:12px; font-size:12px; font-weight:400; }
.sizelink .ad { font-size:17px; vertical-align:text-top; opacity:0.7; }

.group-prod > li { padding:15px 0; display:flex; align-items:center; border-top:1px solid {{settings.color_borders}}; }
.group-prod > li .img { width:70px; font-size:0; }
.group-prod > li .details { padding:0 0 10px 20px; flex:1; display:flex; flex-wrap:wrap; }
.group-prod .product-name { margin-bottom:15px; font-weight:700; font-size:14px; width:100%; }
.group-prod .gprvrAct { color:{{settings.color_text_field_text}}; font-size:13px; padding:7px 10px; cursor:pointer; display:inline-block; position:relative; 
	background-color:{{settings.color_text_field}}; border:1px solid {{settings.field_bd}}; }
.group-prod .gprvrAct span { margin-right:15px; }
.group-prod .gprvrAct .ad { right:10px; top:8px; position:absolute; }
.group-prod .gpprVariants { position:relative; margin-right:10px; }
.group-prod .gpprVariants ul { z-index:11; position:absolute; top:100%; left:0; border:1px solid {{settings.field_bd}}; border-top:0; width:100%; display:none; background-color:{{settings.color_text_field}}; }
.group-prod .gpprVariants li { cursor:pointer; padding:5px 10px; }
.group-prod .gpprVariants li:hover { background-color:{{settings.color_text_field_text | color_modify: 'alpha', 0.05}}; }
.group-prod .qtyField { width:91px; background-color:{{settings.color_text_field}}; }
.group-prod .qtyField .gprQty { padding:0 8px; width:auto; height:32px; line-height:32px; border-radius:0; }
.group-prod .qtyField .qty { line-height:32px; height:32px; width:35px; }
.group-prod .qtyField .ad { font-size:12px; }
.group-prod .no-var { line-height:30px; margin-right:10px; }
.group-prod .money { line-height:33px; margin-right:10px; }
.group-prod .gpprVariants .money { line-height:inherit; }

{% if settings.show_swatch %}
  .product-form .swatch { margin-bottom:15px; padding-left:5px; padding-right:5px; width:100%; }
  .product-form .swatch label.header { margin-bottom:8px; text-transform:uppercase; }

  .swatch-element { display:inline-block; margin:0 4px 2px 0; position:relative; }
  .swatch-element .swatchLbl { color:#333; font-size:12px; font-weight:400; line-height:32px; text-align:center; display:inline-block; margin:0;
      min-width:35px; height:35px; padding:0 10px; background-color:#f5f5f5; border:2px solid transparent;
    {% if settings.sw_color_style == "rounded" %}
      border-radius:20px;
    {% elsif settings.sw_color_style == "rounded_rectangle" %}
      border-radius:5px;
    {% else %}
      border-radius:0;
    {% endif %}
  }
  .swatch-element .swatchLbl.square-only { border-radius:0; }

  .swatch-element .color { padding:0; background-repeat:no-repeat; background-position:{{ settings.v_bg_position}}; background-size:cover;
      border:2px solid #fff; box-shadow:0 0 0 1px #ddd;
      {% if settings.sw_color_size == "medium" %}
          width:50px; height:50px;
      {% elsif settings.sw_color_size == "large" %}
          width:70px; height:70px;
      {% else %}
          width:35px; height:35px;
      {% endif %}
  }
  {% if settings.sw_color_style == "rounded" %}
  .swatch-element .color.rounded { border-radius:50%; }
  {% endif %}
  .swatch-element .swatchInput:checked + .swatchLbl { border:2px solid {{ settings.acti_swatch_bdr_clr}}; box-shadow:none; }
  .swatch-element.soldout .swatchLbl { text-decoration:line-through; opacity:0.5; cursor:no-drop; }
  .swatch-element.soldout .color:after { content:""; display:block; width:100%; height:100%; background:url({{ 'soldout.png' | asset_url}}) no-repeat 50% 50%/cover; }

  @media only screen and (min-width:767px) {
      .swatch-element:hover .tooltip-label { opacity:1; bottom:calc(100% + 7px); visibility:visible; }
  }
{% endif %}

.slVariant { min-width:40px; display:inline-block; font-weight:700; }

{%- if settings.enable_wishlist or settings.askaboutproduct or settings.shippingpopup -%}
.infolinks a.inLink { display:inline-block; vertical-align:top; margin:0 15px 15px 0; color:{{ settings.color_body_text}}; }
.infolinks a.inLink:hover { color:{{ settings.color_body_text}}; opacity:0.8; }
.infolinks .ad { font-size:13px; margin-right:5px; }
.infolinks .btn { display:block; margin-bottom:15px; }
{% endif %}

.product-action { width:100%; position:relative; margin-bottom:15px; flex-wrap:wrap; }
.product-action .qtyField { width:90px; margin:0 10px 0 0; }
.product-action button { max-width:500px; flex:1; height:40px; margin-bottom:0; border:1px solid {{settings.addto_btn_bg}}; background-color:{{settings.addto_btn_bg}}; color:{{settings.addto_btn_text}}; }
.product-action button:hover { background-color:{{settings.addto_btn_bg_hov}}; color:{{settings.addto_btn_text_hov}}; border:1px solid {{settings.addto_btn_text_bdr}};}

{% if settings.enable_payment_button %}
.shopify-payment-button { margin-bottom:30px; max-width:600px; }
.shopify-payment-button .shopify-payment-button__button { 
  background:{{settings.checkout_btn_bg}} !important; color:{{settings.checkout_btn_text}} !important;  text-transform:uppercase; letter-spacing:0.02em;
    {% if settings.btn_style == "round_crn" %}
  	   border-radius:6px;
    {% elsif settings.btn_style == "round"  %}
    	border-radius:25px;
    {% else %}
   		border-radius:0;
    {% endif %}
}
.shopify-payment-button .shopify-payment-button__button--hidden { display:none; }
{% endif %}

