{%- comment -%}*** Site Fonts ***{%- endcomment -%}
{%- liquid
	assign base_font = settings.body_font
    assign base_font_light = base_font | font_modify: 'weight', '300'
    assign base_font_smbold = base_font | font_modify: 'weight', '600'
    assign base_font_bold = base_font | font_modify: 'weight', '700'
    
    if settings.header_font != settings.body_font
    	assign header_font = settings.header_font
        assign head_font_light = header_font | font_modify: 'weight', '300'
        assign head_font_smbold = header_font | font_modify: 'weight', '600'
        assign head_font_bold = header_font | font_modify: 'weight', '700'
    endif
-%}
{%- capture headCss -%}

{% if base_font_light %}<link rel="preload" as="font" href="{{ base_font_light | font_url }}" type="font/woff2" crossorigin>{% endif %}
<link rel="preload" as="font" href="{{ base_font | font_url }}" type="font/woff2" crossorigin>
{% if base_font_smbold %}<link rel="preload" as="font" href="{{ base_font_smbold | font_url }}" type="font/woff2" crossorigin>{% endif %}
{% if base_font_bold %}<link rel="preload" as="font" href="{{ base_font_bold | font_url }}" type="font/woff2" crossorigin>{% endif %}
{%- if settings.header_font != settings.body_font -%}
	{% if head_font_light %}<link rel="preload" as="font" href="{{ head_font_light | font_url }}" type="font/woff2" crossorigin>{% endif %}
    <link rel="preload" as="font" href="{{ header_font | font_url }}" type="font/woff2" crossorigin>
    {% if head_font_smbold %}<link rel="preload" as="font" href="{{ head_font_smbold | font_url }}" type="font/woff2" crossorigin>{% endif %}
    {% if head_font_bold %}<link rel="preload" as="font" href="{{ head_font_bold | font_url }}" type="font/woff2" crossorigin>{% endif %}
{%- endif -%}

