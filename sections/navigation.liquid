    {%- endif -%}
    {%- endif -%}
    {%- endfor -%}
    <li></li><li></li>
    {% if shop.customer_accounts_enabled  %}
    {% if customer %}
    <li><a href="{{ routes.account_url }}">{{ 'customer.account.title' | t }}</a></li>
    <li><a href="{{ routes.account_logout_url }}"><b>{{ 'customer.account.logout' | t }}</b></a></li>
    {% else %}
    <li><a href="{{ routes.account_login_url }}">{{ 'customer.login.form_title' | t }}</a></li>
    {% if shop.customer_accounts_optional %}<li><a href="{{routes.account_register_url}}">{{ 'customer.register.link' | t }}</a></li>{% endif %}
    {% endif %}
    {% endif %}
